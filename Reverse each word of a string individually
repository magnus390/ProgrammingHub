#include<bits/stdc++.h>
using namespace std;
int main()
{
	string str="",s="";
	getline(cin,str);
	for(int i=0;i<str.length();i++)
	{
		if(str[i]!=' ')
		{
			s = s + str[i];
		}
		else
		{
			reverse(s.begin(),s.end());
			cout<<s<<" ";
			s = "";
		}
	}
	reverse(s.begin(),s.end());
	cout<<s<<" ";
	s = "";
return(0);
}
